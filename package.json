{
  "name": "nextai-finance-app-monorepo",
  "private": true,
  "workspaces": [
    "packages/client",
    "packages/server",
    "packages/common"
  ],
  "scripts": {
    "dev": "concurrently \"cd packages/server && npm run dev\" \"cd packages/client && npm run dev:npm\"",
    "dev:server": "cd packages/server && npm run dev",
    "dev:client": "cd packages/client && npm run dev:npm"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^5.22.0"
  },
  "dependencies": {
    "form-data": "^4.0.4"
  }
}
