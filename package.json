{
  "name": "nextai-finance-app-monorepo",
  "private": true,
  "workspaces": [
    "packages/client",
    "packages/server",
    "packages/schema"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=server\"",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "build": "npm run build --workspace=client",
    "build:all": "npm run build --workspace=client",
    "test": "npm run test --workspace=client",
    "lint": "npm run lint --workspace=client"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^5.22.0"
  },
  "dependencies": {
    "form-data": "^4.0.4"
  }
}
